<?xml version='1.0' encoding='UTF-8'?>
<Workflow Id='Dummy'>
  <ConfName>AeroElastAdj</ConfName>
  <Branch Name='Runner'>
    <RunOptions>
      <Data Name='RunMode'>local</Data>
    </RunOptions>
    <DataSection>
      <Data Name='modulename'>Optim</Data>
      <Data Name='pymodule'>worms_OpenDACE.Optimization</Data>
    </DataSection>
    <Branch Name='Optim'>
      <RunOptions>
        <Data Name='RunMode'>local</Data>
      </RunOptions>
      <DataSection>
        <Data Name='import_database'>False</Data>
        <Data Name='Optimization Definition.Multiobjective Optimization'>False</Data>
        <Data Name='Optimization Definition.Constrained Optimization'>True</Data>
        <Data Name='Optimization Definition.Hessian-based'>False</Data>
        <Data Name='Optimization Definition.Gradient-based'>True</Data>
        <Data Name='Evaluator'>worms_AeroElastAdj.LiftingLine</Data>
        <Data Name='Optimizer.Algorithm'>scipy_slsqp</Data>
        <Data Name='Convergence.Override'>False</Data>
        <Data Name='Objective.Expression'>Cd</Data>
        <Data Name='Eq Constraints'>['C0']</Data>
        <Data Name='Ineq Constraints'>[]</Data>
        <Data Name='design_variables'>twist.dat</Data>
        <Data Name='C0.Expression'>Cl-0.5</Data>
      </DataSection>
      <Branch Name='LiftingLine'>
        <RunOptions>
          <Data Name='RunMode'>local</Data>
        </RunOptions>
        <DataSection>
          <Data Name='Evaluation conditions.Mach'>0.5</Data>
          <Data Name='Evaluation conditions.AoA'>5.0</Data>
          <Data Name='Airfoils.Airfoil aerodynamic model'>Linear</Data>
          <Data Name='Airfoils.AoA0'>-2.0</Data>
          <Data Name='Airfoils.Cd0'>0.01</Data>
          <Data Name='Airfoils.Cm'>-0.1</Data>
          <Data Name='Numerical parameters.Relaxation factor'>0.95</Data>
          <Data Name='Numerical parameters.Residual stop criteria'>1e-06</Data>
          <Data Name='Wing geometry.Planform'>Rectangular</Data>
          <Data Name='Wing geometry.Wingspan'>34.1</Data>
          <Data Name='Wing geometry.Root chord'>7.1</Data>
          <Data Name='Wing geometry.Root thickness'>1.28</Data>
          <Data Name='Wing geometry.Tip thickness'>0.33</Data>
          <Data Name='Wing geometry.Number of sections'>40</Data>
          <Data Name='design_variables'>None</Data>
        </DataSection>
      </Branch>
    </Branch>
  </Branch>
</Workflow>
